{
  "$schema": "appsettings-schema.json",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information"
    },
    "WriteTo": [
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "Console"
            }
          ]
        }
      }
    ]
  },
  "Umbraco": {
    "CMS": {
      "Content": {
        "MacroErrors": "Throw"
      },
      "Hosting": {
        "Debug": true
      }
    },
    "Storage": {
      "AzureBlob": {
        "Media": {
          "ConnectionString": "DefaultEndpointsProtocol=http;AccountName=YOUR-ACCOUNT-NAME;AccountKey=YOUR-ACCOUNT-BASE64-KEY;BlobEndpoint=http://127.0.0.1:10000/YOUR-ACCOUNT-NAME;",
          "ContainerName": "YOUR-CONTAINER-NAME",
          "ContainerRootPath": "media",
          "VirtualPath": "~/media"
        },
        "ImageSharp": {
          "ConnectionString": "DefaultEndpointsProtocol=http;AccountName=YOUR-ACCOUNT-NAME;AccountKey=YOUR-ACCOUNT-BASE64-KEY;BlobEndpoint=http://127.0.0.1:10000/YOUR-ACCOUNT-NAME;",
          "ContainerName": "YOUR-CONTAINER-NAME",
          "ContainerRootPath": "cache",
          "VirtualPath": "~/media",
          "CacheRetention": {
            "Enabled": false,
            "NumberOfDays": 90,
            "TestModeEnable": false,
            "TestModeSweepSeconds": 30,
            "TestModeMaxAgeMinutes": 10
          }
        }
      }
    }
  }
}
